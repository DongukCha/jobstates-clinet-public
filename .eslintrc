{
  "extends": "eslint-config-airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-console": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-nested-ternary": "off",
    "react/require-default-props": 0
  },
  "globals": {
    "FormData": false,
    "FileReader": false,
    "window": true,
    "document": true,
    "localStorage": true
  },
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
